    <namespace name="TerraFX.Interop">
        <enumeration name="SpeechTokenContext" access="public">
            <type>int</type>
            <enumerator name="STCInprocServer" access="">
                <type primitive="False">int</type>
                <value>
                    <code>CLSCTX_INPROC_SERVER
        </code>
                </value>
            </enumerator>
            <enumerator name="STCInprocHandler" access="">
                <type primitive="False">int</type>
                <value>
                    <code>CLSCTX_INPROC_HANDLER
        </code>
                </value>
            </enumerator>
            <enumerator name="STCLocalServer" access="">
                <type primitive="False">int</type>
                <value>
                    <code>CLSCTX_LOCAL_SERVER
        </code>
                </value>
            </enumerator>
            <enumerator name="STCRemoteServer" access="">
                <type primitive="False">int</type>
                <value>
                    <code>CLSCTX_REMOTE_SERVER
        </code>
                </value>
            </enumerator>
            <enumerator name="STCAll" access="">
                <type primitive="False">int</type>
                <value>
                    <code>(((CLSCTX_INPROC_SERVER | CLSCTX_INPROC_HANDLER) | CLSCTX_LOCAL_SERVER) | CLSCTX_REMOTE_SERVER)
        </code>
                </value>
            </enumerator>
        </enumeration>
    </namespace>
