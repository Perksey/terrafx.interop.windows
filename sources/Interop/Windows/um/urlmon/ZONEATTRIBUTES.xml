            <field name="cbSize" access="public">
                <type native="ULONG">uint</type>
            </field>
            <field name="szDisplayName" access="public">
                <type native="WCHAR [260]" count="260" fixed="_szDisplayName_e__FixedBuffer">ushort</type>
            </field>
            <field name="szDescription" access="public">
                <type native="WCHAR [200]" count="200" fixed="_szDescription_e__FixedBuffer">ushort</type>
            </field>
            <field name="szIconPath" access="public">
                <type native="WCHAR [260]" count="260" fixed="_szIconPath_e__FixedBuffer">ushort</type>
            </field>
            <field name="dwTemplateMinLevel" access="public">
                <type native="DWORD">uint</type>
            </field>
            <field name="dwTemplateRecommended" access="public">
                <type native="DWORD">uint</type>
            </field>
            <field name="dwTemplateCurrentLevel" access="public">
                <type native="DWORD">uint</type>
            </field>
            <field name="dwFlags" access="public">
                <type native="DWORD">uint</type>
            </field>
        </struct>
