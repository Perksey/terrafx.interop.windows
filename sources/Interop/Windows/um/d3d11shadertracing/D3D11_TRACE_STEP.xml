    <namespace name="TerraFX.Interop">
        <struct name="D3D11_TRACE_STEP" access="public">
            <field name="ID" access="public">
                <type native="UINT">uint</type>
            </field>
            <field name="InstructionActive" access="public">
                <type native="BOOL">int</type>
            </field>
            <field name="NumRegistersWritten" access="public">
                <type native="UINT8">byte</type>
            </field>
            <field name="NumRegistersRead" access="public">
                <type native="UINT8">byte</type>
            </field>
            <field name="MiscOperations" access="public">
                <type native="D3D11_TRACE_MISC_OPERATIONS_MASK">ushort</type>
            </field>
            <field name="OpcodeType" access="public">
                <type native="UINT">uint</type>
            </field>
            <field name="CurrentGlobalCycle" access="public">
                <type native="UINT64">ulong</type>
            </field>
        </struct>
    </namespace>
</bindings>
