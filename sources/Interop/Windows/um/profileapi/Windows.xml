    <namespace name="TerraFX.Interop">
        <class name="Windows" access="public" static="true">
            <function name="QueryPerformanceCounter" access="public" lib="kernel32" setlasterror="true" static="true">
                <type native="BOOL">int</type>
                <param name="lpPerformanceCount">
                    <type>LARGE_INTEGER*</type>
                </param>
            </function>
            <function name="QueryPerformanceFrequency" access="public" lib="kernel32" setlasterror="true" static="true">
                <type native="BOOL">int</type>
                <param name="lpFrequency">
                    <type>LARGE_INTEGER*</type>
                </param>
            </function>
        </class>
    </namespace>
