                <type native="DWORD">uint</type>
            </field>
            <field name="bQuantization" access="public">
                <type native="BYTE">byte</type>
            </field>
            <field name="bNumberOfChannels" access="public">
                <type native="BYTE">byte</type>
            </field>
            <field name="dwReserved" access="public">
                <type native="DWORD [2]" count="2" fixed="_dwReserved_e__FixedBuffer">uint</type>
            </field>
