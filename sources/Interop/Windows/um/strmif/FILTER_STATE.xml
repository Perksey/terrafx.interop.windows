    <namespace name="TerraFX.Interop">
        <enumeration name="FILTER_STATE" access="public">
            <type>int</type>
            <enumerator name="State_Stopped" access="">
                <type primitive="False">int</type>
                <value>
                    <code>0
        </code>
                </value>
            </enumerator>
            <enumerator name="State_Paused" access="">
                <type primitive="False">int</type>
                <value>
                    <code>(State_Stopped + 1)
        </code>
                </value>
            </enumerator>
            <enumerator name="State_Running" access="">
                <type primitive="False">int</type>
                <value>
                    <code>(State_Paused + 1)
        </code>
                </value>
            </enumerator>
        </enumeration>
    </namespace>
