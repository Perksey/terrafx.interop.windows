    <namespace name="TerraFX.Interop">
        <struct name="MFVideoInfo" access="public">
            <field name="dwWidth" access="public">
                <type native="DWORD">uint</type>
            </field>
            <field name="dwHeight" access="public">
                <type native="DWORD">uint</type>
            </field>
            <field name="PixelAspectRatio" access="public">
                <type native="">MFRatio</type>
            </field>
            <field name="SourceChromaSubsampling" access="public">
