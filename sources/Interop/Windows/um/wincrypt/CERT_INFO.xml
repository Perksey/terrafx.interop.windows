    </comment>
    <namespace name="TerraFX.Interop">
        <struct name="CERT_INFO" access="public" unsafe="true">
            <field name="dwVersion" access="public">
                <type native="DWORD">uint</type>
            </field>
            <field name="SerialNumber" access="public">
                <type native="CRYPT_INTEGER_BLOB">CRYPTOAPI_BLOB</type>
            </field>
            <field name="SignatureAlgorithm" access="public">
                <type native="">CRYPT_ALGORITHM_IDENTIFIER</type>
            </field>
            <field name="Issuer" access="public">
                <type native="CERT_NAME_BLOB">CRYPTOAPI_BLOB</type>
            </field>
            <field name="NotBefore" access="public">
                <type native="">FILETIME</type>
            </field>
            <field name="NotAfter" access="public">
                <type native="">FILETIME</type>
            </field>
            <field name="Subject" access="public">
                <type native="CERT_NAME_BLOB">CRYPTOAPI_BLOB</type>
            </field>
            <field name="SubjectPublicKeyInfo" access="public">
                <type native="">CERT_PUBLIC_KEY_INFO</type>
            </field>
            <field name="IssuerUniqueId" access="public">
                <type native="">CRYPT_BIT_BLOB</type>
            </field>
            <field name="SubjectUniqueId" access="public">
                <type native="">CRYPT_BIT_BLOB</type>
            </field>
            <field name="cExtension" access="public">
                <type native="DWORD">uint</type>
            </field>
            <field name="rgExtension" access="public">
                <type native="PCERT_EXTENSION">CERT_EXTENSION*</type>
            </field>
        </struct>
    </namespace>
</bindings>
