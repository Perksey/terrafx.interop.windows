    <namespace name="TerraFX.Interop">
        <struct name="CMSG_RECIPIENT_ENCRYPTED_KEY_ENCODE_INFO" access="public" unsafe="true">
            <field name="cbSize" access="public">
                <type native="DWORD">uint</type>
            </field>
            <field name="RecipientPublicKey" access="public">
                <type native="">CRYPT_BIT_BLOB</type>
            </field>
            <field name="RecipientId" access="public">
                <type native="">CERT_ID</type>
            </field>
            <field name="Date" access="public">
                <type native="">FILETIME</type>
            </field>
            <field name="pOtherAttr" access="public">
                <type native="PCRYPT_ATTRIBUTE_TYPE_VALUE">CRYPT_ATTRIBUTE_TYPE_VALUE*</type>
            </field>
        </struct>
    </namespace>
