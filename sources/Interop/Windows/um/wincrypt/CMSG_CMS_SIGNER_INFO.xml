    <namespace name="TerraFX.Interop">
        <struct name="CMSG_CMS_SIGNER_INFO" access="public">
            <field name="dwVersion" access="public">
                <type native="DWORD">uint</type>
            </field>
            <field name="SignerId" access="public">
                <type native="">CERT_ID</type>
            </field>
            <field name="HashAlgorithm" access="public">
                <type native="">CRYPT_ALGORITHM_IDENTIFIER</type>
            </field>
            <field name="HashEncryptionAlgorithm" access="public">
                <type native="">CRYPT_ALGORITHM_IDENTIFIER</type>
            </field>
            <field name="EncryptedHash" access="public">
                <type native="CRYPT_DATA_BLOB">CRYPTOAPI_BLOB</type>
            </field>
            <field name="AuthAttrs" access="public">
                <type native="">CRYPT_ATTRIBUTES</type>
            </field>
            <field name="UnauthAttrs" access="public">
                <type native="">CRYPT_ATTRIBUTES</type>
            </field>
        </struct>
    </namespace>
