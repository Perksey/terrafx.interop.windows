    <namespace name="TerraFX.Interop">
        <struct name="CERT_REQUEST_INFO" access="public" unsafe="true">
            <field name="dwVersion" access="public">
                <type native="DWORD">uint</type>
            </field>
            <field name="Subject" access="public">
                <type native="CERT_NAME_BLOB">CRYPTOAPI_BLOB</type>
            </field>
            <field name="SubjectPublicKeyInfo" access="public">
                <type native="">CERT_PUBLIC_KEY_INFO</type>
            </field>
            <field name="cAttribute" access="public">
                <type native="DWORD">uint</type>
            </field>
            <field name="rgAttribute" access="public">
                <type native="PCRYPT_ATTRIBUTE">CRYPT_ATTRIBUTE*</type>
            </field>
        </struct>
    </namespace>
