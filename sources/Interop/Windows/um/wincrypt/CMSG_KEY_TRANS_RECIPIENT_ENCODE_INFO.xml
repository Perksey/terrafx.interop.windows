    <namespace name="TerraFX.Interop">
        <struct name="CMSG_KEY_TRANS_RECIPIENT_ENCODE_INFO" access="public" unsafe="true">
            <field name="cbSize" access="public">
                <type native="DWORD">uint</type>
            </field>
            <field name="KeyEncryptionAlgorithm" access="public">
                <type native="">CRYPT_ALGORITHM_IDENTIFIER</type>
            </field>
            <field name="pvKeyEncryptionAuxInfo" access="public">
                <type native="void *">void*</type>
            </field>
            <field name="hCryptProv" access="public">
                <type native="HCRYPTPROV_LEGACY">nuint</type>
            </field>
            <field name="RecipientPublicKey" access="public">
                <type native="">CRYPT_BIT_BLOB</type>
            </field>
            <field name="RecipientId" access="public">
                <type native="">CERT_ID</type>
            </field>
        </struct>
    </namespace>
