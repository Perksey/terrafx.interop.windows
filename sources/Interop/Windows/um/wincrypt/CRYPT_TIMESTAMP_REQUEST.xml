    <namespace name="TerraFX.Interop">
        <struct name="CRYPT_TIMESTAMP_REQUEST" access="public" unsafe="true">
            <field name="dwVersion" access="public">
                <type native="DWORD">uint</type>
            </field>
            <field name="HashAlgorithm" access="public">
                <type native="">CRYPT_ALGORITHM_IDENTIFIER</type>
            </field>
            <field name="HashedMessage" access="public">
                <type native="CRYPT_DER_BLOB">CRYPTOAPI_BLOB</type>
            </field>
            <field name="pszTSAPolicyId" access="public">
                <type native="LPSTR">sbyte*</type>
            </field>
            <field name="Nonce" access="public">
                <type native="CRYPT_INTEGER_BLOB">CRYPTOAPI_BLOB</type>
            </field>
            <field name="fCertReq" access="public">
                <type native="BOOL">int</type>
            </field>
            <field name="cExtension" access="public">
                <type native="DWORD">uint</type>
            </field>
            <field name="rgExtension" access="public">
                <type native="PCERT_EXTENSION">CERT_EXTENSION*</type>
            </field>
        </struct>
    </namespace>
</bindings>
