    <namespace name="TerraFX.Interop">
        <struct name="CTL_INFO" access="public" unsafe="true">
            <field name="dwVersion" access="public">
                <type native="DWORD">uint</type>
            </field>
            <field name="SubjectUsage" access="public">
                <type native="">CTL_USAGE</type>
            </field>
            <field name="ListIdentifier" access="public">
                <type native="CRYPT_DATA_BLOB">CRYPTOAPI_BLOB</type>
            </field>
            <field name="SequenceNumber" access="public">
                <type native="CRYPT_INTEGER_BLOB">CRYPTOAPI_BLOB</type>
            </field>
            <field name="ThisUpdate" access="public">
                <type native="">FILETIME</type>
            </field>
            <field name="NextUpdate" access="public">
                <type native="">FILETIME</type>
            </field>
            <field name="SubjectAlgorithm" access="public">
                <type native="">CRYPT_ALGORITHM_IDENTIFIER</type>
            </field>
            <field name="cCTLEntry" access="public">
                <type native="DWORD">uint</type>
            </field>
            <field name="rgCTLEntry" access="public">
                <type native="PCTL_ENTRY">CTL_ENTRY*</type>
            </field>
            <field name="cExtension" access="public">
                <type native="DWORD">uint</type>
            </field>
            <field name="rgExtension" access="public">
                <type native="PCERT_EXTENSION">CERT_EXTENSION*</type>
            </field>
        </struct>
    </namespace>
</bindings>
