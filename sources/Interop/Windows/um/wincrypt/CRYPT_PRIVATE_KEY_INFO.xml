    <namespace name="TerraFX.Interop">
        <struct name="CRYPT_PRIVATE_KEY_INFO" access="public" unsafe="true">
            <field name="Version" access="public">
                <type native="DWORD">uint</type>
            </field>
            <field name="Algorithm" access="public">
                <type native="">CRYPT_ALGORITHM_IDENTIFIER</type>
            </field>
            <field name="PrivateKey" access="public">
                <type native="CRYPT_DER_BLOB">CRYPTOAPI_BLOB</type>
            </field>
            <field name="pAttributes" access="public">
                <type native="PCRYPT_ATTRIBUTES">CRYPT_ATTRIBUTES*</type>
            </field>
        </struct>
    </namespace>
</bindings>
