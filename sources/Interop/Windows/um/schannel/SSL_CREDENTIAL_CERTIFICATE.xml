    <namespace name="TerraFX.Interop">
        <struct name="SSL_CREDENTIAL_CERTIFICATE" access="public" unsafe="true">
            <field name="cbPrivateKey" access="public">
                <type native="DWORD">uint</type>
            </field>
            <field name="pPrivateKey" access="public">
                <type native="PBYTE">byte*</type>
            </field>
            <field name="cbCertificate" access="public">
                <type native="DWORD">uint</type>
            </field>
            <field name="pCertificate" access="public">
                <type native="PBYTE">byte*</type>
            </field>
            <field name="pszPassword" access="public">
