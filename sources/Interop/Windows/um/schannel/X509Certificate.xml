    <namespace name="TerraFX.Interop">
        <struct name="X509Certificate" access="public" unsafe="true">
            <field name="Version" access="public">
                <type native="DWORD">uint</type>
            </field>
            <field name="SerialNumber" access="public">
                <type native="DWORD [4]" count="4" fixed="_SerialNumber_e__FixedBuffer">uint</type>
            </field>
            <field name="SignatureAlgorithm" access="public">
                <type native="ALG_ID">uint</type>
            </field>
            <field name="ValidFrom" access="public">
                <type native="">FILETIME</type>
            </field>
            <field name="ValidUntil" access="public">
                <type native="">FILETIME</type>
            </field>
            <field name="pszIssuer" access="public">
                <type native="PSTR">sbyte*</type>
            </field>
            <field name="pszSubject" access="public">
                <type native="PSTR">sbyte*</type>
            </field>
            <field name="pPublicKey" access="public">
                <type native="PctPublicKey *">PctPublicKey*</type>
            </field>
        </struct>
    </namespace>
</bindings>
