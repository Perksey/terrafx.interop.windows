    <namespace name="TerraFX.Interop">
        <struct name="SEALING_SIGNATURE_ATTRIBUTE" access="public">
            <field name="version" access="public">
                <type native="ULONG">uint</type>
            </field>
            <field name="signerIndex" access="public">
                <type native="ULONG">uint</type>
            </field>
            <field name="signatureAlgorithm" access="public">
                <type native="">CRYPT_ALGORITHM_IDENTIFIER</type>
            </field>
            <field name="encryptedDigest" access="public">
                <type native="CRYPT_DIGEST_BLOB">CRYPTOAPI_BLOB</type>
            </field>
