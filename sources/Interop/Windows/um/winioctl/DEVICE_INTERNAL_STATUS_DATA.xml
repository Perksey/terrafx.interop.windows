    <namespace name="TerraFX.Interop">
        <struct name="DEVICE_INTERNAL_STATUS_DATA" access="public" unsafe="true">
            <field name="Version" access="public">
                <type native="DWORD">uint</type>
            </field>
            <field name="Size" access="public">
                <type native="DWORD">uint</type>
            </field>
            <field name="T10VendorId" access="public">
                <type native="DWORDLONG">ulong</type>
            </field>
            <field name="DataSet1Length" access="public">
                <type native="DWORD">uint</type>
            </field>
            <field name="DataSet2Length" access="public">
                <type native="DWORD">uint</type>
            </field>
            <field name="DataSet3Length" access="public">
                <type native="DWORD">uint</type>
            </field>
            <field name="DataSet4Length" access="public">
                <type native="DWORD">uint</type>
            </field>
            <field name="StatusDataVersion" access="public">
                <type native="BYTE">byte</type>
            </field>
            <field name="Reserved" access="public">
                <type native="BYTE [3]" count="3" fixed="_Reserved_e__FixedBuffer">byte</type>
            </field>
            <field name="ReasonIdentifier" access="public">
                <type native="BYTE [128]" count="128" fixed="_ReasonIdentifier_e__FixedBuffer">byte</type>
            </field>
            <field name="StatusDataLength" access="public">
                <type native="DWORD">uint</type>
            </field>
            <field name="StatusData" access="public">
                <type native="BYTE [1]" count="1" fixed="_StatusData_e__FixedBuffer">byte</type>
            </field>
        </struct>
    </namespace>
